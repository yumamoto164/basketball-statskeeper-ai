{
  "name": "basketball-statskeeper-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "cd backend && python src/main.py",
    "dev": "npm run dev:frontend & npm run dev:backend",
    "build:frontend": "npm run build --workspace=frontend",
    "build": "npm run build:frontend",
    "install:all": "npm install && npm install --workspace=frontend",
    "install:backend": "cd backend && pip install -r requirements.txt"
  },
  "description": "Monorepo for Basketball Stats Keeper AI - Frontend React app and Backend agentic code",
  "keywords": [
    "basketball",
    "stats",
    "ai",
    "monorepo"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "file-saver": "^2.0.5"
  },
  "devDependencies": {
    "@types/file-saver": "^2.0.7",
    "react-icons": "^5.5.0"
  }
}
